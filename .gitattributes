# Auto detect text files and perform LF normalization
*        text=auto  eol=lf

*.cs     text diff=csharp
*.java   text diff=java
*.html   text diff=html
*.py     text diff=python
*.pl     text diff=perl
*.pm     text diff=perl
*.css    text
*.js     text
*.sql    text
*.c      text
*.cpp    text
*.h      text
*.hpp    text
*.dts    text
*.cmd    text 

*.sh     text eol=lf

*.bat    text eol=crlf
*.vcxproj text merge=union eol=crlf
*.csproj text merge=union eol=crlf
*.sln    text merge=union eol=crlf

*.dtb    binary
*.dtbo   binary
*.out    binary

